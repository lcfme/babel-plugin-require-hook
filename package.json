{
  "name": "babel-plugin-require-hook",
  "version": "0.0.1",
  "description": "A babel plugin to call custom function when babel requires a module",
  "repository": {
    "type": "git",
    "url": "https://github.com/lcfme/babel-plugin-require-hook"
  },
  "main": "lib/index.js",
  "scripts": {
    "compile": "cross-env NODE_ENV=production babel src/index.js --out-dir lib",
